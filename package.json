{
  "name": "chepi-front",
  "version": "1.0.1",
  "description": "Frontend for chepi",
  "author": "https://github.com/Allohamora",
  "private": true,
  "license": "UNLICENSED",
  "main": "",
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "npm run lint src test -- --fix",
    "csslint": "stylelint",
    "csslint:fix": "npm run csslint \"**/*.tsx\" -- --fix",
    "format": "prettier",
    "format:fix": "prettier --write .",
    "test": "jest",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.4",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "antd": "^4.17.0-alpha.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "next": "11.1.2",
    "next-translate": "^1.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.21.1",
    "string-similarity": "^4.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@emotion/babel-plugin": "^11.3.0",
    "@types/jest": "^27.0.1",
    "@types/react": "17.0.19",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-beautiful-sort": "^1.0.6",
    "eslint-plugin-prettier": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.1.0",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-rational-order-fix": "^0.1.8",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "4.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Allohamora/chepi-front.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/Allohamora/chepi-front/issues"
  },
  "homepage": "https://github.com/Allohamora/chepi-front#readme",
  "engines": {
    "node": "^14.17.4",
    "npm": "^6.14.15"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint -- --fix",
      "npm run test -- --bail --findRelatedTests"
    ],
    "*.tsx": [
      "npm run csslint -- --fix"
    ],
    "*.{js,json,yml,md}": [
      "npm run format -- --write"
    ]
  }
}
