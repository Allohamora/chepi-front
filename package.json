{
  "name": "chepi-front",
  "version": "3.0.0",
  "description": "front-end for chepi",
  "author": "https://github.com/Allohamora",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "csslint": "stylelint \"**/*.tsx\"",
    "csslint:fix": "stylelint \"**/*.tsx\" --fix",
    "format": "prettier . --check",
    "format:fix": "prettier --write .",
    "test": "jest",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "antd": "4.18.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "next": "^12.1.0",
    "next-translate": "^1.3.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.34.13",
    "string-similarity": "^4.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@emotion/babel-plugin": "^11.7.2",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@types/jest": "^27.4.0",
    "@types/react": "17.0.39",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-beautiful-sort": "^2.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.0",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "stylelint": "^14.3.0",
    "stylelint-config-rational-order-fix": "^0.1.9",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "4.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Allohamora/chepi-front.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/Allohamora/chepi-front/issues"
  },
  "homepage": "https://github.com/Allohamora/chepi-front#readme",
  "engines": {
    "node": ">=16.13.2",
    "npm": ">=8.1.2"
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "jest --bail --findRelatedTests"
    ],
    "*.tsx": [
      "stylelint --fix"
    ],
    "*.{js,json,yml,md}": [
      "prettier --write"
    ]
  }
}
