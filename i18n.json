{
  "locales": ["en", "ru", "uk"],
  "defaultLocale": "en",
  "pages": {
    "*": ["search-layout", "cookie", "city", "country", "language"],
    "/": ["home"],
    "/404": ["not-found"],
    "/500": ["error"],
    "/stats": ["stats", "alert-status"],
    "/search/[country]/[city]": [
      "search-view",
      "comparison-toggle-button",
      "more-info-link",
      "buy-link",
      "value",
      "alert-status"
    ],
    "/comparison/[type]": ["comparison", "value", "more-info-link", "buy-link", "property", "alert-status"],
    "/pizza/[id]": ["pizza", "comparison-toggle-button", "more-info-link", "buy-link", "value", "alert-status"]
  }
}
